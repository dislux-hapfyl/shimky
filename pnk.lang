wwc() {
    file1="$1"
    file2="$2"
    count1=$(wc -c < "$file1")
    count2=$(wc -c < "$file2")
    
    echo -e "\nCharacter count comparison:"
    echo "$1 "
    for ((i=0; i<$count1; i+=100)); do
        printf "X"
    done
    echo " ($count1)"
    
    echo "$2 "
    for ((i=0; i<$count2; i+=100)); do
        printf "U"
    done
    echo " ($count2)"
}
