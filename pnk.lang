####### Head, Body & Tail #######
ZDimp() {
	typeset ii="$1"
        $E "import $ii"
}
ZDas() {
	typeset ii="$1"
	typeset aa="$2"
        $E "import $ii as $aa"
}
ZDshebang() {
	$E "#!/usr/bin/env python3"
	$E "#pnk.lang.gen;"
}
ZDfrm() {
	typeset lib="$1"
	typeset cclass="$2"
	$E "from $lib import $cclass"
}
#root window
ZFtoorwin() {
	typeset pp="$1"
	ZDdc $pp Tk
	YDfd z __init__ 
	XFfc - "super().__init__"
	XFcfg '' bg=$bg
}
#class instance
XIcv() {
	typeset name="$1"
	typeset cclass="$2"
	typeset s="$3"
	XIfv z $name $cclass self,$s
}
WIcv() {
	typeset name="$1"
	typeset cclass="$2"
	typeset s="$3"
	WIfv z $name $cclass self,$s
}
#class define
ZDcld() {
	typeset nn="$1"
	typeset xx="$2"
	typeset pp="$3"
	ZDdc $nn Frame
	YDfd z __init__ ,parent,$xx
	XFfc - "super().__init__" parent,$pp
	XIv z master parent
	XFcfg '' bg=$bg
}
ZFmain() {
	typeset pp="$1"
	typeset windowTitle="$2"
	$E "if __name__ == '__main__':"
  	YIfv - go $pp
  	YFfc - go.title "\"$windowTitle\""
  	YFfc - go.mainloop
}

